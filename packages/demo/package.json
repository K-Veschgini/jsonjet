{
  "name": "resonancedb-demo",
  "version": "0.1.0",
  "private": true,
  "description": "Interactive demo application for ResonanceDB - a stream processing database",
  "type": "module",
  "main": "src/index.js",
  "module": "src/index.js",
  "scripts": {
    "dev": "bun --hot src/index.js",
    "dev:no-hot": "bun src/index.js",
    "build": "bun build ./src/index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*'",
    "start": "NODE_ENV=production bun src/index.js"
  },
  "dependencies": {
    "@resonancedb/core": "workspace:*",
    "@resonancedb/ui": "workspace:*",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/bun": "latest"
  }
}
