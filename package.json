{
  "name": "resonancedb",
  "version": "1.0.0",
  "description": "ResonanceDB - A stream processing database with flow-based architecture",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "bun run main.js",
    "build": "bun run build.js",
    "build:single": "bun build main.js --outfile=release/resonancedb.js --target=node",
    "build:bundle": "bun build main.js --outdir=release --target=node --splitting --minify",
    "test": "bun test tests/",
    "test:summarize": "bun test tests/summarize.test.js",
    "test:watch": "bun test tests/ --watch",
    "test-release": "bun run test-release.js",
    "release": "bun run clean && bun run build && bun run test-release",
    "clean": "rm -rf release"
  },
  "keywords": ["resonancedb", "streaming", "flow-based", "data-processing", "operators", "scan", "filter", "pipeline"],
  "author": "",
  "license": "MIT",
  "devDependencies": {},
  "files": [
    "src/",
    "main.js",
    "README.md"
  ],
  "dependencies": {
    "chevrotain": "^11.0.3"
  }
}